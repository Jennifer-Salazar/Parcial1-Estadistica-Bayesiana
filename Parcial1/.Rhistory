library(ggplot2)
x <- seq(0, 10000, by=100)
y <- seq(0, 100, by=1)
datos <- data.frame(x,y)
par(pty="s")
ggplot(datos, aes(x,y))+
scale_y_continuous(breaks=seq(0, 100, 10), labels = c("Absolutamente improbable", "Me parece muy improbable", "Me parece algo improbable", "No me genera seguridad", "Es más bien algo incierto", "Ni acuerdo ni en contra(50-50)", "Pudiera ser, pero...", "Me parece algo posible", "Me parece demasiado viable", "Estoy casi seguro", "Absolutamente seguro"), name=" ") +
scale_x_continuous(breaks=seq(0, 10000, 250), name=" ")+
theme(axis.text.x = element_text(angle = 90, vjust = 0.5, hjust=1))+
geom_vline(xintercept=seq(0, 10000, 250), color="gray")+
geom_hline(yintercept=seq(0, 100, 10), color="gray")+
ggtitle ("Número de barberos que hay en la ciudad de Medellín")+
theme(plot.title = element_text(hjust = 0.5))
